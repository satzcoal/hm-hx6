<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
  <title>UCTS - 官方网站</title>
  <script type="text/javascript" src="http://code.jquery.com/jquery-1.11.0.js"></script>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
</head>
<body>
<div class="main">
  <div class="header-menu">
    <div class="header-menu-bar"></div>
  </div>
  <div class="content-show" style="top: 20%; left: 30%;">
    <div class="img-div">
      <!--<img src="/img/contact.png" class="img-show" style="width: 60%"/>-->
    </div>
  </div>
</div>
<div class="sidebar-show">
  <div class="sidebar-logo">
    <div class="sidebar-logo-left"></div>
    <div class="sidebar-logo-right" style="width: 250px">
      <span class="sidebar-logo-main">CONTACT</span><span class="sidebar-logo-sub">联系我们</span>
    </div>
  </div>
  <div class="sidebar-container">
    <div class="sidebar-container-line">
    </div>
    <div class="sidebar-container-left">
    </div>
    <div class="sidebar-container-right">
      <div class="sidebar-item sidebar-footer">
        <a href="/">
          <div class="sidebar-footer-arrow">
          </div>
          <div class="sidebar-item-word-group">
            <span class="sidebar-footer-en">HOME</span>
            <span class="sidebar-footer-cn">首页</span>
          </div>
        </a>
      </div>
    </div>
  </div>
</div>
</body>
</html>
